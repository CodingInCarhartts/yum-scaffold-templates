[package]
name = "{{project_name}}"
version = "0.1.0"
edition = "2021"
description = "{{description}}"

[dependencies]
tokio = { version = "1.48.0", features = ["full"] }
serde = { version = "1.0.228", features = ["derive"] }
serde_json = "1.0.145"
{{#if add_logging}}
log = "0.4"
env_logger = "0.11"
{{/if}}
anyhow = "1.0"
clap = { version = "4.5", features = ["derive"] }
chrono = { version = "0.4.42", features = ["serde"] }
